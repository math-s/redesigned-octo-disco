[default.deploy.parameters]
stack_name = "ygpp"
region = "us-east-1"
capabilities = "CAPABILITY_IAM"
confirm_changeset = true
disable_rollback = true
resolve_s3 = true

# Replace with your GitHub Pages origin.
parameter_overrides = [
  "AllowedOrigin=https://math-s.github.io",

  # Do NOT commit your real token. Keep it local in backend/samconfig.toml
  # or pass it at deploy time with --parameter-overrides.
  "AdminToken=REPLACE_ME",
]
